{
  "version": 2,
  "builds": [
    {
      "src": "app.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "app.js"
    },
    {
      "src": "/static/(.*)", 
      "dest": "static/$1" 
    },
    {
      "src": "/script/(.*)",
      "dest": "script/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "app.js" le
    }
  ]
}

